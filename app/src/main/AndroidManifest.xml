<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qq.smsparser">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".view.order.OrderGoodMessageAty"></activity>

        <activity android:name=".view.send.SendGoodMessageAty">

        </activity>

        <activity android:name=".view.analysis.SaleMessageAty"></activity>
        <activity android:name=".view.helper.HelperMessageAty"></activity>

        <service
            android:name=".controller.parser.SmsService"
            android:process=".Service">
            <intent-filter>
                <action android:name="com.example.qq.server"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </service>

    </application>

</manifest>